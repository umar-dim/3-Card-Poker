<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loseController">
    <children>
         <Label layoutX="225.0" layoutY="61.0" style="-fx-font-size: 36px; -fx-font-weight: bold;" text="You Lose!" />
      <VBox layoutX="201.0" layoutY="229.0" prefHeight="160.0" prefWidth="198.0">
         <children>
               <Button fx:id="playAgainButton" alignment="CENTER" contentDisplay="CENTER" onAction="#playAgainButtonPressed" prefHeight="25.0" prefWidth="272.0" text="Play Again" textAlignment="CENTER" />
               <Button fx:id="exitButton" alignment="CENTER" contentDisplay="CENTER" onAction="#exitButtonPressed" prefHeight="25.0" prefWidth="255.0" text="Exit" textAlignment="CENTER" />
         </children>
      </VBox>
          <Label layoutX="147.0" layoutY="169.0" text="Earnings:" />
          <TextField fx:id="earningsBox" editable="false" layoutX="220.0" layoutY="165.0" />
    </children>
</AnchorPane>
